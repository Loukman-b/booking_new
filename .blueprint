created:
    - database/factories/PackageFactory.php
    - database/factories/TimeSlotFactory.php
    - database/factories/BookingFactory.php
    - database/factories/PaymentFactory.php
    - database/factories/ExpenseFactory.php
    - database/migrations/2026_02_04_135230_create_packages_table.php
    - database/migrations/2026_02_04_135231_create_time_slots_table.php
    - database/migrations/2026_02_04_135232_create_bookings_table.php
    - database/migrations/2026_02_04_135233_create_payments_table.php
    - database/migrations/2026_02_04_135234_create_expenses_table.php
    - app/Models/Package.php
    - app/Models/TimeSlot.php
    - app/Models/Booking.php
    - app/Models/Payment.php
    - app/Models/Expense.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable', two_factor_secret: 'text nullable', two_factor_recovery_codes: 'text nullable', two_factor_confirmed_at: 'datetime nullable' }
    Package: { name: string, description: text, price: 'decimal:8,2', duration_minutes: integer, active: 'boolean default:true', timestamps: timestamps }
    TimeSlot: { package_id: 'id foreign', start_time: datetime, end_time: datetime, status: 'enum:available,booked,cancelled default:available', timestamps: timestamps }
    Booking: { package_id: 'id foreign', time_slot_id: 'id foreign', customer_name: string, customer_email: string, customer_phone: 'string nullable', price_at_booking: 'decimal:8,2', status: 'enum:confirmed,cancelled,completed default:confirmed', timestamps: timestamps }
    Payment: { booking_id: 'id foreign', amount: 'decimal:8,2', payment_method: 'string nullable', paid_at: 'datetime nullable', timestamps: timestamps }
    Expense: { description: string, amount: 'decimal:8,2', expense_date: date, created_by: 'id foreign:users.id', timestamps: timestamps }
